    <title>Mahavir's profile</title>
    <script>
        $('document').ready(function(){
            $('.btn').click(function(){
                $('#cont').hide();
                $('#contact').show();
            });
            $('#contact').click(function(){
                $('#cont').show();
                $('#contact').hide();
            });
        });
    </script>
